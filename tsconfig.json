{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@config": [
        "./config/index.ts"
      ],
      "@api-v1/*": [
        "./api/v1/*"
      ],
    },
    // "isolatedModules": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    // "rootDir": "./src"
  },
  "include": [
    "src/**/*",
    "src/api/v1/**/*"
, "ecosystem.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}